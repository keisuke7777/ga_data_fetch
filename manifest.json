{
  "manifest_version": 2,
  "name": "GAクローラー",
  "version": "1.0",
  "description": "Google Analyticsのユーザーアクテビティを取得するChrome拡張機能",

  "browser_action": {
    "default_icon": {
      "16": "sushi_akami.png"
    },
    "default_popup": "popup.html",
    "all_frames": true
  },
  "background": {
    "scripts": ["background.js"]
  },

  "permissions": ["activeTab", "background", "storage", "unlimitedStorage"]
}
